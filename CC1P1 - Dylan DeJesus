// Capsule Code
public class Capsule : MonoBehaviour
{
    int capsuleInteger = 5;
    // Start is called before the first frame update
    void Start()
    {
        float capsuleFloat = 1.5f;
        
        capsuleInteger = 11;

        Debug.Log(capsuleInteger);
        Debug.Log(capsuleFloat);
    }

    // Update is called once per frame
    void Update()
    {
       
    }
}

// Sphere Code
public class Sphere : MonoBehaviour
{

    bool isSphereTrue = false;
    
    // Start is called before the first frame update
    void Start()
    {
        isSphereTrue = true;

    }

    // Update is called once per frame
    void Update()
    {
        Debug.Log(isSphereTrue);
    }
}

// Cube Code
public class Cube : MonoBehaviour
{

    string firstString = "Gaming";
    string secondString = "is";
    string thirdString = "Life";
    // Start is called before the first frame update
    void Start()
    {
        Debug.Log(firstString);
        Debug.Log(secondString);
        Debug.Log(thirdString);

        Debug.Log(firstString + " " + secondString + " " + thirdString);
    }

    // Update is called once per frame
    void Update()
    {
        
    }
}
